  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "bundle-require": "^3.1.0",
    "commander": "^9.4.1",
    "debug": "^4.3.4",
    "esbuild": "^0.14.54",
    "fuelchain": "workspace:*",
    "fuels": "workspace:*",
    "joycon": "^3.1.1",
    "typechain-target-fuels": "workspace:*",
    "typescript": "^4.8.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/glob": "^7.2.0",
    "glob": "^8.0.3",
    "tsup": "^6.2.3"
  }
